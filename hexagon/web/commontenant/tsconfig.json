{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@galileo/web_commontenant/app/_core": [
        "dist/web_commontenant/app/_core"
      ],
      "@galileo/web_commontenant/adapter": [
        "dist/web_commontenant/adapter"
      ],
      "@galileo/web_commontenant/_common": [
        "dist/web_commontenant/_common"
      ],
      "@galileo/web_commontenant/app/admin/data-sharing": [
        "dist/web_commontenant/app/admin/data-sharing"
      ],
      "@galileo/web_commontenant/app/admin/activity-monitoring": [
        "dist/web_commontenant/app/admin/activity-monitoring"
      ],
      "@galileo/web_commontenant/app/admin/tenant-configuration": [
        "dist/web_commontenant/app/admin/tenant-configuration"
      ],
      "@galileo/web_commontenant/app/admin/tenant-management": [
        "dist/web_commontenant/app/admin/tenant-management"
      ],
      "@galileo/web_commontenant/app/admin/onboarding": [
        "dist/web_commontenant/app/admin/onboarding"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}