{
  "name": "web_commonidentity",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn set-cert && ng serve",
    "build": "yarn set-cert && yarn build:lib --configuration production",
    "build:lib": "yarn validate && ng build web_commonidentity",
    "build:login-provider-element": "ng build login-provider-element --aot --build-optimizer --extract-licenses --no-namedChunks --optimization --no-sourceMap --no-vendorChunk && yarn cat:login-provider-element",
    "cat:login-provider-element": "jscat dist/login-provider-element/runtime.js dist/login-provider-element/polyfills.js dist/login-provider-element/main.js > dist/commonidentity-login-provider_element.js",
    "build:testbed": "ng build --aot",
    "test": "ng test --no-watch --project=web_commonidentity",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "validate": "yarn galileo -t importValidator -e @galileo/web_commonidentity",
    "set-cert": "git update-index --assume-unchanged angular.json && yarn galileo -t certUpdater",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.2",
    "@angular/common": "~13.3.2",
    "@angular/compiler": "~13.3.2",
    "@angular/core": "~13.3.2",
    "@angular/elements": "^13.3.2",
    "@angular/forms": "~13.3.2",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "~13.3.2",
    "@angular/platform-browser-dynamic": "~13.3.2",
    "@angular/router": "~13.3.2",
    "@galileo/platform_common-http": "^1.1.3",
    "@galileo/platform_commonidentity": "^1.0.15",
    "core-js": "^2.5.4",
    "country-list": "^2.1.1",
    "file-saver": "^2.0.2",
    "ngx-chips": "^2.2.2",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "yarn": "^1.21.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cdk": "^13.3.2",
    "@angular/cli": "^13.3.2",
    "@angular/compiler-cli": "~13.3.2",
    "@angular/language-service": "~13.3.2",
    "@angular/material": "^13.3.2",
    "@galileo/common-tools": "latest",
    "@galileo/web_common-http": "latest",
    "@galileo/web_common-libraries": "latest",
    "@galileo/web_commonconversations": "latest",
    "@galileo/web_commonfeatureflags": "latest",
    "@galileo/web_commonlayoutmanager": "latest",
    "@galileo/web_commonlicensing": "latest",
    "@galileo/web_commonlocalization": "latest",
    "@galileo/web_commonmap": "latest",
    "@galileo/web_commonrecovery": "latest",
    "@galileo/web_commontenant": "latest",
    "@galileo/web_layoutbuilder-common": "latest",
    "@galileo/web_model-viewer": "latest",
    "@ngx-translate/core": "^13.0.0",
    "@types/country-list": "^2.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
