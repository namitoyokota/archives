{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "es2020",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2015",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2018",
            "dom"
        ],
        "paths": {
            "@galileo/web_commonnotifications/_core": [
                "dist/web_commonnotifications/_core"
            ],
            "@galileo/web_commonnotifications/adapter": [
                "dist/web_commonnotifications/adapter"
            ],
            "@galileo/web_commonnotifications/_common": [
                "dist/web_commonnotifications/_common"
            ],
            "@galileo/web_commonnotifications/admin": [
                "dist/web_commonnotifications/admin"
            ]
        }
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    }
}