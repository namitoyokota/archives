{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@galileo/web_commonkeywords/_core": [
        "dist/web_commonkeywords/_core",
      ],
      "@galileo/web_commonkeywords/admin": [
        "dist/web_commonkeywords/admin",
      ],
      "@galileo/web_commonkeywords/adapter": [
        "dist/web_commonkeywords/adapter",
      ],
      "@galileo/web_commonkeywords/_common": [
        "dist/web_commonkeywords/_common",
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}