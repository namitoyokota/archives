  "scripts": {
    "ng": "ng",
    "start": "yarn set-cert && ng serve",
    "build": "yarn set-cert && yarn build:lib --configuration production",
    "test": "ng test --no-watch --project=web_alarms",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "yarn validate && ng build web_alarms",
    "build:testBed": "ng build --aot",
    "publish": "npm publish ./dist/web_alarms",
    "validate": "yarn galileo -t importValidator -e @galileo/web_alarms",
    "set-cert": "git update-index --assume-unchanged angular.json && yarn galileo -t certUpdater",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
