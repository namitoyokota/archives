{
  "name": "@galileo/mobile_commonidentity",
  "version": "1.0.5",
  "description": "test",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "galileo-mobile-commonidentity.podspec",
    "!lib/typescript/testbed",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "testbed": "yarn --cwd testbed",
    "bootstrap": "yarn testbed && yarn && yarn testbed pods",
    "fileending": "npx crlf --set=LF  node_modules/.bin/mobilegalileo",
    "commontool": "yarn mobilegalileo -t directoryCopier -di ./",
    "testbedtool": "yarn mobilegalileo -t directoryCopier -di ./ -de ./testbed/src"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://hexagon-si-gpc.visualstudio.com/",
  "author": "Jeremy Leshko <jeremy.leshko@hexagon.com> (https://hexagon-si-gpc.visualstudio.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://hexagon-si-gpc.visualstudio.com//issues"
  },
  "homepage": "https://hexagon-si-gpc.visualstudio.com/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/eslint-parser": "^7.18.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@galileo/mobile_dynamic-injection-engine": "latest",
    "@galileo/platform_common-http": "^1.1.13",
    "@galileo/platform_common-libraries": "^1.0.7",
    "@galileo/platform_commonidentity": "^1.0.6",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/eslint-config": "^3.0.2",
    "@react-native-cookies/cookies": "^6.2.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "@galileo/mobile_commonlibraries": "latest",
    "@galileo/mobile-common-tools": "latest",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.68.0",
    "@types/react-native-crypto-js": "^1.0.0",
    "@types/styled-components-react-native": "^5.1.3",
    "commitlint": "^17.0.2",
    "country-list": "^2.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-builder-bob": "^0.18.3",
    "react-native-crypto-js": "1.0.0",
    "react-native-webview": "^11.23.1",
    "release-it": "^15.0.0",
    "rxjs": "6.5.2",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/testbed/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@babel/eslint-parser",
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "styled-components": "^5.3.6"
  }
}
