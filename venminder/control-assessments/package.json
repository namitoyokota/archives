    "build:e2ehotfix": "webpack --env envSetting=E2E-HOTFIX",
    "build:e2emaster": "webpack --env envSetting=E2E-MASTER",
    "build:e2eintegration": "webpack --env envSetting=E2E-INTEGRATION",
    "build:staging": "webpack --env envSetting=STAGING",
    "build:prod": "webpack --env envSetting=PROD",
    "analyze": "webpack --env PROD --analyze",
    "test": "au test",
    "T4": "Build-T4-WithAUBuild.sh",
    "auth": "vsts-npm-auth -config .npmrc"
