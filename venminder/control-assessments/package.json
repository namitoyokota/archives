  "scripts": {
    "build": "webpack --env envSetting=PROD",
    "start": "webpack server",
    "build:dev": "npm run T4 && webpack --env envSetting=dev",
    "build:gets": "webpack --env envSetting=GETS",
    "build:hotfix": "webpack --env envSetting=HOTFIX",
    "build:integration": "webpack --env envSetting=INTEGRATION",
