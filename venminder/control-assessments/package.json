    "aurelia-templating": "1.11.1",
    "aurelia-validation": "2.0.0-rc2",
    "bootstrap": "3.4.1",
    "date-fns": "^2.29.3",
    "diacritic": "0.0.2",
    "dompurify": "2.4.0",
    "eonasdan-bootstrap-datetimepicker": "4.17.49",
    "fast-equals": "3.0.1",
